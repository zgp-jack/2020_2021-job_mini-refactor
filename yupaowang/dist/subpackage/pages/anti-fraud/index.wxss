.anti-fraudbox {
  position: relative;
  width: 100%;
  padding: 20rpx 27rpx; }
.anti-fraud-item {
  position: relative;
  width: 100%; }
.anti-fraud-title {
  position: relative;
  font-size: 34rpx;
  color: #f40521;
  font-weight: bold;
  line-height: 80rpx; }
.anti-fraud-tips {
  position: relative;
  display: block;
  font-size: 30rpx;
  color: #f40521;
  line-height: 60rpx;
  padding-left: 50rpx; }
.anti-fraud-tips::before {
  position: absolute;
  content: '';
  display: block;
  width: 36rpx;
  height: 36rpx;
  background: url("http://cdn.yupao.com/miniprogram/images/laba.png") no-repeat;
  background-size: 100% 100%;
  left: 0;
  top: 50%;
  margin-top: -15rpx; }
.anti-fraud-info,
.anti-fraud-other {
  font-size: 28rpx;
  line-height: 50rpx;
  color: #555; }
.anti-fraud-other {
  color: #f40521; }