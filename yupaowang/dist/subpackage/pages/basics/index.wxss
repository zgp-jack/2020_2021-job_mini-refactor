.overwords {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; }
.showMore {
  position: relative;
  width: 100%;
  padding: 20rpx 0;
  text-align: center;
  font-size: 26rpx;
  color: #525252; }
.clearfix:after {
  content: "";
  display: block;
  clear: both;
  visibility: hidden;
  line-height: 0;
  height: 0;
  font-size: 0; }
.clearfix {
  zoom: 1; }
.header {
  width: 93%;
  margin: 0 auto;
  background: white;
  font-size: 30rpx;
  color: #f87a14;
  text-align: center;
  line-height: 80rpx;
  margin-bottom: 20rpx; }
.publish-recruit-card {
  position: relative;
  width: 93%;
  margin: 0 auto;
  background-color: white; }
.publish-list-item {
  display: flex;
  position: relative;
  width: 90%;
  height: 125rpx;
  line-height: 125rpx;
  margin-left: 30rpx;
  border-bottom: 1rpx solid #F1F1F1; }
.publish-list-item-sex {
  display: flex;
  position: relative;
  width: 90%;
  height: 125rpx;
  line-height: 125rpx;
  border-bottom: 1rpx solid #F1F1F1; }
.pulish-list-title {
  width: 180rpx;
  display: block;
  text-align: justify;
  line-height: 125rpx;
  font-size: 30rpx; }
.pulish-list-title::after {
    content: '.';
    width: 100%;
    display: inline-block;
    overflow: hidden;
    height: 0; }
.publish-list-input {
  position: relative;
  width: 100%;
  height: 125rpx;
  line-height: 125rpx;
  display: block;
  font-size: 30rpx;
  margin-left: 30rpx; }
.publish-list-input .publish-input-list {
    position: relative;
    display: inline-block;
    margin-right: 20rpx; }
.publish-list-input .publish-input-list:nth-last-child(1) {
      margin-right: 0; }
.footer {
  width: 100%; }
.footer-btn {
  margin: 0 auto;
  width: 521rpx;
  background: #108EEF;
  color: white;
  font-size: 38rpx;
  text-align: center;
  line-height: 97rpx;
  margin-top: 20rpx;
  margin-bottom: 42rpx; }
.publish-list-ditals {
  padding: 30rpx;
  font-size: 30rpx; }
.publish-code-btn {
  position: absolute;
  line-height: 70rpx;
  border-radius: 10rpx;
  background-color: #0099ff;
  color: white;
  font-size: 28rpx;
  text-align: center;
  right: 0;
  top: 20rpx;
  width: 250rpx; }
.pulish-list-location {
  position: absolute;
  line-height: 125rpx;
  right: 20rpx;
  top: 0;
  color: #09f;
  font-size: 28rpx;
  border-left: 0 solid #8f8f8f;
  z-index: 100; }
.publish-list-item-phone {
  display: flex;
  position: relative;
  width: 90%;
  height: 125rpx;
  line-height: 125rpx;
  margin: 20rpx 0 20rpx 30rpx; }
.publish-list-item-isCode {
  display: flex;
  position: relative;
  width: 90%;
  height: 125rpx;
  line-height: 125rpx;
  margin: 20rpx 0 0 30rpx;
  border-bottom: 1rpx solid #F1F1F1; }
.publish-list-item-code {
  display: flex;
  position: relative;
  width: 90%;
  height: 125rpx;
  line-height: 125rpx;
  margin: 20rpx 0 20rpx 30rpx; }
.textarea {
  height: 100rpx !important; }