<block wx:if="{{$taroCompReady}}">
    <view class="rankingRules-content">
        <block wx:if="{{isLogin}}">
            <auth></auth>
        </block>
        <view class="rankingRules-top">
            <view class="rankingRules-top-ranking">我的排名点：<text class="rankingRules-top-ranking-num">{{data.resume_info.sort_flag}}</text>
            </view>
            <view class="rankingRules-top-careful">注：找活名片只有审核通过后，才会在列表展示。排名点越高，排名越靠前。</view>
        </view>
        <view class="rankingRules-list">
            <view class="rankingRules-list-box" wx:key="$loopState__temp2" wx:for="{{loopArray117}}" wx:for-item="v" wx:for-index="index">
                <view class="rankingRules-list-content">
                    <image class="rankingRules-list-content-img" src="{{v.$original.img_url}}"></image>
                    <view class="rankingRules-list-content-box">
                        <view>{{v.$original.type_name}}</view>
                        <view class="rankingRules-list-content-box-text">{{v.$original.description}}</view>
                    </view>
                    <block>
                        <block wx:if="{{v.$original.is_share != 1 && !showbutton}}">
                            <view class="{{v.$original.jump.need_jump == 0 ? 'rankingRules-list-content-buttonBox-none' : 'rankingRules-list-content-buttonBox'}}" bindtap="anonymousFunc0" data-e-tap-so="this" data-e-tap-a-a="{{v._$indexKey}}">{{v.$original.button_words}}</view>
                        </block>
                        <block wx:else>
                            <view class="rankingRules-list-content-buttonBox" bindtap="anonymousFunc1" data-e-tap-so="this" data-e-tap-a-a="{{v._$indexKey2}}">{{v.$original.button_words}}</view>
                        </block>
                    </block>
                </view>
            </view>
        </view>
        <view class="rankingRules-Tips">
            <view class="rankingRules-Tips-title">温馨提示</view>
            <view class="rankingRules-Tips-text" wx:key="$loopState__temp4" wx:for="{{loopArray118}}" wx:for-item="v" wx:for-index="index">{{v.$original}}</view>
        </view>
    </view>
</block>