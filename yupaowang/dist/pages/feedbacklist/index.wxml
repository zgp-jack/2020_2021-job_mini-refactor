<block wx:if="{{$taroCompReady}}">
    <view>
        <auth></auth>
        <wechat-notice></wechat-notice>
        <block wx:if="{{!lists.item.length}}">
            <nodata compid="{{$compid__68}}"></nodata>
        </block>
        <view class="recruit-lists-containerbox">
            <block wx:if="{{lists.item}}">
                <block wx:key="id" wx:for="{{loopArray79}}" wx:for-item="item" wx:for-index="__index1">
                    <view class="feedback-body-content">
                        <view class="superior">
                            <view class="superior-text"><text>问</text>
                            </view>
                            <view class="superior-content"><text>{{item.$original.content}}</text>
                            </view>
                        </view>
                        <view class="species">
                            <block wx:if="{{item.$original.images}}">
                                <view class="species-box" wx:key="$loopState__temp2" bindtap="anonymousFunc0" wx:for="{{item.$anonymousCallee__16}}" wx:for-item="v" wx:for-index="index" data-e-tap-so="this" data-e-tap-a-a="{{v._$indexKey}}">
                                    <image class="species-box-img" src="{{v.$original}}"></image>
                                </view>
                            </block>
                        </view>
                        <view class="species-text">{{item.$original.ask_time}}</view>
                        <block wx:if="{{item.$original.is_answer}}">
                            <view class="superior-data">
                                <view class="superiordati">答</view>
                                <view class="superioredase">
                                    <block wx:if="{{item.$original.send_msg}}"><text>{{item.$original.send_msg}}</text>
                                    </block>
                                </view>
                            </view>
                        </block>
                    </view>
                </block>
            </block>
            <block wx:if="{{!isDown && lists.item.length}}">
                <view class="feedback-noData">没有更多数据了</view>
            </block>
        </view>
        <view class="feedback-bttonBox">
            <button class="feedback-bttonBox-botton" bindtap="anonymousFunc2">我要提意见</button>
        </view>
    </view>
</block>