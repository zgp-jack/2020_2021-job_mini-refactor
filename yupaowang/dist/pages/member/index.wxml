<block wx:if="{{$taroCompReady}}">
    <view class="member-container">
        <view class="member-header-box">
            <view class="member-header-title">会员中心</view>
            <view class="member-userinfobox">
                <block>
                    <block wx:if="{{login && model}}">
                        <view class="member-userinfo">
                            <view class="member-userinfo-content">
                                <image class="member-userinfo-avatar" src="http://cdn.yupao.com/miniprogram/images/user.png"></image>
                                <view class="member-username">
                                    <view class="member-username-text">{{model.member.username || model.member.nickname}}
                                        <block wx:if="{{model.is_checking == 2 && model.member.is_check == '2'}}">
                                            <image class="member-realnameimg" src="http://cdn.yupao.com/miniprogram/images/newresume-infolist-ysm.png"></image>
                                        </block>
                                    </view>
                                </view>
                                <view class="member-usernum">会员编号：<text class="member-id">{{model.member.id}}</text>
                                </view>
                                <block>
                                    <block wx:if="{{model.member.tel}}">
                                        <view class="member-editinfo" bindtap="anonymousFunc0">修改资料</view>
                                    </block>
                                    <block wx:else>
                                        <view class="member-editinfo" bindtap="anonymousFunc1">完善资料</view>
                                    </block>
                                </block>
                            </view>
                            <view class="member-user-integral">
                                <view class="member-integral-item"><text class="member-integral-num">{{model.member.integral}}</text><text class="member-integral-text">积分</text>
                                </view>
                                <view class="member-integral-item"><text class="member-integral-num">{{model.member.temporary_integral}}</text><text class="member-integral-text">临时积分</text><text class="member-integral-tips">详情</text>
                                </view>
                            </view>
                        </view>
                    </block>
                    <block wx:else>
                        <view class="member-userinfo member-userinfo-null" bindtap="anonymousFunc2">
                            <view class="member-toauth">
                                <image class="member-user-nullimg" src="{{IMGCDNURL + 'userauth-userinfo-null.png'}}"></image><text class="member-user-login">登录/注册</text>
                            </view>
                            <view class="member-login-tips">点击上方登录/注册按钮可以前往登录页面，登录成功后可解锁小程序全部功能，方便您能更便捷的招工找活。</view>
                        </view>
                    </block>
                </block>
            </view>
        </view>
        <view class="member-body-container">
            <view class="member-list-container">
                <view class="member-list-item">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-recruit.png'}}"></image><text class="member-list-title">我的招工</text>
                    <block wx:if="{{jobNumber}}"><text class="member-list-tips">状态有更新</text>
                    </block>
                </view>
                <view class="member-list-item">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-resume.png'}}"></image><text class="member-list-title">我的找活名片</text><text class="member-list-tips">{{model && model.member.resume_status.resume_tips_string}}</text>
                </view>
                <view class="member-list-item">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-used.png'}}"></image><text class="member-list-title">我的二手交易</text>
                </view>
                <view class="member-list-item">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-info.png'}}"></image>
                    <view class="member-list-title"><text>我的信息</text>
                        <block wx:if="{{msgNumber}}">
                            <view class="member-num-dotbox"><text class="member-num-dot">{{msgNumber > 9 ? '9+' : msgNumber}}</text>
                            </view>
                        </block>
                    </view>
                    <block wx:if="{{msgNumber}}"><text class="member-list-tips">有新信息待查看</text>
                    </block>
                </view>
            </view>
            <view class="member-list-container">
                <view class="member-list-item" bindtap="anonymousFunc3">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-integral.png'}}"></image><text class="member-list-title">获取积分</text>
                    <block wx:if="{{!ios}}"><text class="member-list-tips">去充值</text>
                    </block>
                </view>
                <view class="member-list-item" bindtap="anonymousFunc4">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-invite.png'}}"></image><text class="member-list-title">邀请工友</text><text class="member-list-tips">邀请好友得积分</text>
                </view>
                <view class="member-list-item">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-expend.png'}}"></image><text class="member-list-title">积分消耗记录</text>
                </view>
                <view class="member-list-item">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-origin.png'}}"></image><text class="member-list-title">积分来源</text>
                </view>
            </view>
            <view class="member-list-container">
                <view class="member-list-item" bindtap="anonymousFunc5">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-realname.png'}}"></image><text class="member-list-title">实名认证</text><text class="member-list-tips">{{model && model.member.check_state}}</text>
                </view>
                <view class="member-list-item">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-collect.png'}}"></image><text class="member-list-title">我的收藏</text>
                </view>
            </view>
            <view class="member-list-container">
                <view class="member-list-item">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-feedback.png'}}"></image>
                    <view class="member-list-title"><text>意见反馈</text>
                        <block wx:if="{{model && model.member.has_notice_msg.hasNoticeMsgg}}"><text class="member-list-dot"></text>
                        </block>
                    </view>
                    <block wx:if="{{model && model.member.has_notice_msg.hasNoticeMsgg}}"><text class="member-list-tips">有最新回复</text>
                    </block>
                </view>
                <view class="member-list-item">
                    <image class="member-list-icon" src="{{IMGCDNURL + 'lpy/ucenter/newcenter-help.png'}}"></image><text class="member-list-title">帮助中心</text><text class="member-list-tips">使用教程</text>
                </view>
            </view>
        </view>
    </view>
</block>