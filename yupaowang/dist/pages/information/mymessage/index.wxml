<block wx:if="{{$taroCompReady}}">
    <view>
        <auth></auth>
        <block wx:if="{{!data.item.length}}">
            <nodata></nodata>
        </block>
        <view class="messsage-lists" wx:key="$loopState__temp2" bindtap="anonymousFunc0" wx:for="{{loopArray91}}" wx:for-item="item" wx:for-index="index" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey}}">
            <view class="messsage-lists-top">
                <view class="messsage-num">
                    <image src="{{item.$original.imageUrl}}" class="messsage-num-img"></image>
                    <block wx:if="{{item.$original.count > 0}}"><text class="msgNumber">{{item.$original.count > 9 ? '9+' : item.$original.count}}</text>
                    </block>
                </view>
                <view class="messsage-title">
                    <view class="msgflex"><text class="msgflex-text"> {{item.$original.typeName}} </text>
                        <image src="{{IMGCDNURL + 'new-center-rightarrow.png'}}" class="rightarrow"></image>
                    </view>
                    <view class="messsage-deail">{{item.$original.description}}</view>
                </view>
            </view>
            <view class="messsage-bottom"><text class="messsage-deail"> 立即查看 </text><text class="messsage-deail">  {{item.$original.timeString}} </text>
            </view>
        </view>
    </view>
</block>