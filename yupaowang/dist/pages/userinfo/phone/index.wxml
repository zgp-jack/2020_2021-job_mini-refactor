<block wx:if="{{$taroCompReady}}">
    <view class="userchangephone-container">
        <view class="userchangephone-body">
            <view class="publish-list-item"><text class="pulish-list-title">手机号</text>
                <input class="publish-list-input" type="number" placeholder="请输入您的手机号" value="{{info.tel}}" bindinput="anonymousFunc0" />
            </view>
            <view class="publish-list-item publish-list-item-code"><text class="pulish-list-title">验证码</text>
                <input class="publish-list-input" type="number" placeholder="请输入验证码" value="{{info.code}}" bindinput="anonymousFunc1" />
                <view class="publish-code-btn" bindtap="anonymousFunc2">{{text}}</view>
            </view>
        </view>
        <view class="user-submit-btn" bindtap="anonymousFunc3">确定更换</view>
    </view>
</block>