.overwords {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; }
.showMore {
  position: relative;
  width: 100%;
  padding: 20rpx 0;
  text-align: center;
  font-size: 26rpx;
  color: #525252; }
.clearfix:after {
  content: "";
  display: block;
  clear: both;
  visibility: hidden;
  line-height: 0;
  height: 0;
  font-size: 0; }
.clearfix {
  zoom: 1; }
.user-published-container {
  position: relative; }
.user-published-container .user-published-header {
    position: fixed;
    width: 100%;
    left: 0;
    top: 0;
    background: white;
    height: 100rpx;
    z-index: 10;
    border-bottom: 1rpx solid #dcdcdc;
    box-shadow: 0 0 10rpx #e7e6e4;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center; }
.user-published-container .user-published-header .user-published-header-item {
      position: relative;
      flex: 1;
      text-align: center; }
.user-published-container .user-published-header .user-published-header-item .published-item-title {
      position: relative;
      display: inline-block;
      font-size: 28rpx;
      line-height: 100rpx;
      border-bottom: 5rpx solid transparent; }
.user-published-container .user-published-header .user-published-header-item.user-published-header-item-active {
      position: relative; }
.user-published-container .user-published-header .user-published-header-item.user-published-header-item-active .published-item-title {
        color: #0099ff;
        border-bottom: 5rpx solid #0099ff; }
.user-published-container .user-published-body {
    position: fixed;
    width: 100%;
    left: 0;
    top: 100rpx;
    z-index: 20;
    height: calc(100% - 115rpx); }
.user-published-container .user-published-body .user-published-item {
      position: relative;
      padding: 20rpx 20rpx 0;
      background: white;
      margin-top: 20rpx;
      box-shadow: 0 0 10rpx #e7e6e4; }
.user-published-container .user-published-body .user-published-item .user-published-title {
        position: relative;
        width: 100%;
        font-size: 28rpx;
        font-weight: bold;
        line-height: 70rpx;
        color: #333; }
.user-published-container .user-published-body .user-published-item .user-published-content {
        position: relative;
        width: 100%;
        overflow: hidden;
        text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        word-break: break-all;
        font-size: 28rpx;
        color: #6B6B6B;
        line-height: 45rpx;
        max-height: 90rpx;
        margin-bottom: 15rpx; }
.user-published-container .user-published-footer {
    position: relative;
    padding: 20rpx 0;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    border-top: 1rpx solid #dcdcdc;
    justify-content: space-between; }
.user-published-container .user-published-footer .user-published-footer-item {
      position: relative;
      width: 160rpx;
      display: inline-block;
      border: 1rpx solid #dcdcdc;
      border-radius: 5rpx;
      font-size: 28rpx;
      color: #525252;
      line-height: 60rpx;
      text-align: center; }
.user-published-container .user-published-footer .published-ischeking {
      position: relative;
      padding-left: 60rpx;
      line-height: 60rpx;
      font-size: 28rpx;
      color: #ff0000; }
.user-published-container .user-published-footer .published-ischeking .published-checking-img {
        position: absolute;
        display: inline-block;
        width: 30rpx;
        height: 30rpx;
        border-radius: 50%;
        top: 15rpx;
        left: 0; }
.user-published-container .published-top-time {
    position: relative;
    font-size: 28rpx; }
.user-published-container .published-status-img {
    position: absolute;
    right: 0;
    top: -7rpx;
    width: 100rpx;
    height: 40rpx;
    z-index: 2; }
.published-top-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 10rpx;
  margin: auto 0; }
.published-top-cancel {
  background: #F5F5F5;
  color: #F12424;
  font-size: 30rpx;
  display: inline-block;
  line-height: 50rpx;
  padding: 0 10rpx;
  width: 160rpx;
  border: 1rpx solid #F5F5F5;
  text-align: center; }