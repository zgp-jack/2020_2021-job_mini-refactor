<block wx:if="{{$taroCompReady}}">
    <view class="user-published-container">
        <view class="user-published-header">
            <view bindtap="anonymousFunc0" class="{{item.$loopState__temp2}}" wx:for="{{loopArray122}}" wx:for-item="item" wx:for-index="__index0" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey}}"><text class="published-item-title">{{item.$original.title}}</text>
            </view>
        </view>
        <scroll-view class="user-published-body" scroll-y="{{true}}" refresher-enabled="{{true}}" refresher-triggered="{{refresh}}" bindrefresherrefresh="anonymousFunc1" lower-threshold="{{200}}" bindscrolltolower="anonymousFunc2">
            <view class="user-published-item" wx:key="id" wx:for="{{loopArray123}}" wx:for-item="item" wx:for-index="index">
                <block wx:if="{{item.$original.is_check == '1'}}">
                    <image class="published-status-img" src="{{IMGCDNURL + 'published-recruit-checking.png'}}"></image>
                </block>
                <block wx:if="{{item.$original.is_check == '0'}}">
                    <image class="published-status-img" src="{{IMGCDNURL + 'published-recruit-nopass.png'}}"></image>
                </block>
                <block wx:if="{{item.$original.is_end == '2'}}">
                    <image class="published-status-img" src="{{IMGCDNURL + 'published-recruit-end.png'}}"></image>
                </block>
                <view bindtap="anonymousFunc3" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey2}}">
                    <view class="user-published-title overwords">{{item.$original.title}}</view>
                    <view class="user-published-content">{{item.$original.detail}}</view>
                </view>
                <view class="user-published-footer">
                    <block wx:if="{{item.$original.is_check == '1'}}">
                        <view class="published-ischeking">
                            <image class="published-checking-img" src="{{IMGCDNURL + 'published-info.png'}}"></image>提示：人工审核中，该信息仅自己可见。</view>
                    </block>
                    <block wx:if="{{item.$original.is_check != '1'}}">
                        <view class="user-published-footer-item">修改</view>
                    </block>
                    <block wx:if="{{item.$original.is_check == '2'}}">
                        <block>
                            <view class="user-published-footer-item" bindtap="anonymousFunc4" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey3}}">{{item.$original.is_end == '2' ? '重新招工' : '停止招工'}}</view>
                            <block wx:if="{{item.$original.is_end != '2'}}">
                                <view>
                                    <block>
                                        <block wx:if="{{item.$original.top && item.$original.top_data && item.$original.top_data.is_top == '1'}}">
                                            <view class="user-published-footer-item" bindtap="anonymousFunc5" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey4}}">取消置顶</view>
                                        </block>
                                        <block wx:else>
                                            <view class="user-published-footer-item" bindtap="anonymousFunc6" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey5}}">我要置顶</view>
                                        </block>
                                    </block>
                                </view>
                            </block>
                        </block>
                    </block>
                </view>
                <block wx:if="{{item.$original.top && item.$original.top_data && item.$original.top_data.is_top == '1'}}">
                    <view class="published-top-box">
                        <view class="published-top-time">到期时间：2020年04月30日11:31:38</view>
                        <view class="published-top-cancel" bindtap="anonymousFunc7" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey6}}">修改置顶</view>
                    </view>
                </block>
            </view>
            <block wx:if="{{!more && searchData.page > 1}}">
                <view class="showMore">没有更多数据了</view>
            </block>
            <block wx:if="{{!lists.length}}">
                <view class="user-published-nodata">
                    <nodata></nodata>
                </view>
            </block>
        </scroll-view>
    </view>
</block>