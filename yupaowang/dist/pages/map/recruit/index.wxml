<block wx:if="{{$taroCompReady}}">
    <view class="publishrecruit-container">
        <view class="mapinfo-header">
            <view class="mapinfo-header-area" bindtap="anonymousFunc0">{{area}}</view>
            <view class="mapinfo-header-inputbox">
                <input class="mapinfo-header-input" bindtap="anonymousFunc1" bindinput="anonymousFunc2" placeholder="请输入您的详细地址" value="{{smAreaText}}" /><text class="at-icon at-icon-close mapinfo-header-clear"></text>
            </view><text class="mapinfo-header-close" bindtap="anonymousFunc3">取消</text>
        </view>
        <view class="mapinfo-container">
            <view class="location-lists">
                <block wx:if="{{showHistory}}">
                    <view class="history-title">历史记录</view>
                </block>
                <block>
                    <block wx:if="{{showHistory}}">
                        <view class="location-list-item" wx:key="$loopState__temp2" bindtap="anonymousFunc4" wx:for="{{loopArray122}}" wx:for-item="item" wx:for-index="index" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey}}">
                            <view class="location-list-header overwords">{{item.$original.name}}<text>{{item.$original.distance}}</text>
                            </view>
                            <view class="location-list-content">{{item.$original.district}}</view>
                        </view>
                    </block>
                    <block wx:else>
                        <view class="location-list-item" wx:key="$loopState__temp4" bindtap="anonymousFunc5" wx:for="{{loopArray123}}" wx:for-item="item" wx:for-index="index" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey2}}">
                            <view class="location-list-header overwords">{{item.$original.name}}<text>{{item.$original.distance}}</text>
                            </view>
                            <view class="location-list-content">{{item.$original.district}}</view>
                        </view>
                    </block>
                </block>
                <block wx:if="{{showHistory ? !histroyList.length : !lists.length}}">
                    <view class="location-nodata">
                        <image class="location-nodata-img" src="{{IMGCDNURL + 'nodata.png'}}"></image>
                        <view class="location-nodata-text">暂无数据</view>
                    </view>
                </block>
            </view>
        </view>
        <block wx:if="{{showCity}}">
            <view class="mapinfo-citys">
                <cities compid="{{$compid__113}}"></cities>
            </view>
        </block>
    </view>
</block>