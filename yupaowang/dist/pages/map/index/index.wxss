.overwords {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; }
.showMore {
  position: relative;
  width: 100%;
  padding: 20rpx 0;
  text-align: center;
  font-size: 26rpx;
  color: #525252; }
.clearfix:after {
  content: "";
  display: block;
  clear: both;
  visibility: hidden;
  line-height: 0;
  height: 0;
  font-size: 0; }
.clearfix {
  zoom: 1; }
.mapinfo-header {
  position: fixed;
  padding-left: 20rpx;
  background-color: white;
  height: 100rpx;
  padding-right: 100rpx;
  display: table;
  width: 100%;
  z-index: 20;
  left: 0;
  top: 0;
  border-bottom: 1rpx solid #dcdcdc;
  box-shadow: 0 0 10rpx #e7e6e4; }
.mapinfo-header .mapinfo-header-area {
    position: relative;
    display: table-cell;
    font-size: 28rpx;
    white-space: nowrap;
    line-height: 100rpx;
    padding-right: 40rpx; }
.mapinfo-header .mapinfo-header-area::after {
      display: block;
      width: 20rpx;
      height: 20rpx;
      content: '';
      border-top: 3rpx solid #999;
      border-right: 3rpx solid #999;
      position: absolute;
      -webkit-transform: rotate(135deg);
              transform: rotate(135deg);
      right: 20rpx;
      top: 33rpx; }
.mapinfo-header .mapinfo-header-inputbox {
    position: relative;
    display: table-cell;
    height: 100%;
    vertical-align: middle; }
.mapinfo-header .mapinfo-header-inputbox .mapinfo-header-input {
      position: relative;
      display: block;
      width: 100%;
      height: 80rpx;
      line-height: 80rpx;
      border: 1rpx solid #dcdcdc;
      border-radius: 5rpx;
      padding: 0 10rpx;
      padding-right: 70rpx;
      font-size: 30rpx; }
.mapinfo-header .mapinfo-header-inputbox .mapinfo-header-clear {
      position: absolute;
      right: 20rpx;
      top: 0;
      line-height: 100rpx;
      font-size: 30rpx; }
.mapinfo-header .mapinfo-header-close {
    position: absolute;
    right: 20rpx;
    font-size: 32rpx;
    line-height: 100rpx;
    top: 0;
    color: #969494; }
.mapinfo-container {
  position: relative;
  padding-top: 115rpx;
  min-height: 100%;
  height: 100%; }
.mapinfo-container .location-lists {
    position: relative;
    min-height: 100%;
    background-color: white; }
.mapinfo-container .location-lists .history-title {
      position: relative;
      font-size: 26rpx;
      font-weight: bold;
      color: #333;
      border-bottom: 1rpx solid #dcdcdc;
      line-height: 60rpx;
      padding: 20rpx; }
.mapinfo-container .location-lists .location-list-item {
      position: relative;
      padding: 10rpx 20rpx;
      border-bottom: 1rpx solid #dcdcdc; }
.mapinfo-container .location-lists .location-list-item:nth-last-child(1) {
        border-bottom: none; }
.mapinfo-container .location-lists .location-list-item .location-list-header {
        position: relative;
        padding-right: 200rpx;
        line-height: 70rpx;
        font-size: 32rpx;
        color: #333; }
.mapinfo-container .location-lists .location-list-item .location-list-header text {
          position: absolute;
          right: 0;
          top: 0; }
.mapinfo-container .location-lists .location-list-item .location-list-content {
        position: relative;
        padding-bottom: 10rpx;
        line-height: 50rpx;
        font-size: 28rpx;
        color: #666; }
.mapinfo-container .location-lists .location-nodata {
      position: relative;
      padding: 120rpx 0;
      background-color: white;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center; }
.mapinfo-container .location-lists .location-nodata .location-nodata-img {
        position: relative;
        display: block;
        width: 300rpx;
        height: 300rpx; }
.mapinfo-container .location-lists .location-nodata .location-nodata-text {
        position: relative;
        font-size: 28rpx;
        color: #333;
        margin-top: 20rpx; }
.mapinfo-citys {
  position: fixed;
  width: 100%;
  height: 100%;
  background: #f7f6f9;
  left: 0;
  top: 0;
  z-index: 20; }