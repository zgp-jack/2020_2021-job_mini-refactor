<block wx:if="{{$taroCompReady}}">
    <block>
        <view class="realname-container">
            <view class="realname-cardimgbox">
                <view class="realname-card-title">请拍摄并上传你的身份证照片</view>
                <view class="realname-card-imgs clearfix">
                    <view class="realname-card-item" bindtap="anonymousFunc0">
                        <view class="realname-card-img">
                            <image class="realname-card-upimg" src="{{model && model.idCardImg ? ALIYUNCDN + (model && model.idCardImg) : IMGCDNURL + 'lpy/auth/idcard-l.png'}}"></image>
                        </view>
                        <view class="realname-card-title">身份证正面照</view>
                    </view>
                    <view class="realname-card-item" bindtap="anonymousFunc1">
                        <view class="realname-card-img">
                            <image class="realname-card-upimg" src="{{model && model.handImg ? ALIYUNCDN + (model && model.handImg) : IMGCDNURL + 'lpy/auth/idcard-z.png'}}"></image>
                        </view>
                        <view class="realname-card-title">手持身份证照</view>
                    </view>
                </view>
            </view>
            <view class="realname-card-remark">
                <view class="realname-remark-title">拍摄身份证要求</view>
                <view class="realname-remark-tips">大陆公民持有的本人有效二代身份证;</view>
                <view class="realname-remark-tips">拍摄时确保身份证<text class="realname-remark-text">边框完整，字体清晰，亮度均匀;</text>
                </view>
                <view class="realname-remark-imglist">
                    <view class="realname-remark-imgitem">
                        <image class="realname-remark-item-img" src="{{IMGCDNURL + 'lpy/auth/idcard-yes.png'}}"></image><text class="realname-remark-item-text">标准</text>
                    </view>
                    <view class="realname-remark-imgitem">
                        <image class="realname-remark-item-img" src="{{IMGCDNURL + 'lpy/auth/idcard-bkqs.png'}}"></image><text class="realname-remark-item-text">边框缺失</text>
                    </view>
                    <view class="realname-remark-imgitem">
                        <image class="realname-remark-item-img" src="{{IMGCDNURL + 'lpy/auth/idcard-qs.png'}}"></image><text class="realname-remark-item-text">照片模糊</text>
                    </view>
                    <view class="realname-remark-imgitem">
                        <image class="realname-remark-item-img" src="{{IMGCDNURL + 'lpy/auth/idcard-sg.png'}}"></image><text class="realname-remark-item-text">闪光强烈</text>
                    </view>
                </view>
            </view>
            <view class="realname-card-form">
                <view class="publish-list-item"><text class="pulish-list-title">姓名</text>
                    <input class="publish-list-input" type="text" placeholder="请输入姓名" bindinput="anonymousFunc2" value="{{model && model.username}}" />
                </view>
                <view class="publish-list-item"><text class="pulish-list-title">性别</text>
                    <picker bindchange="anonymousFunc3" value="{{sexCurrent}}" range-key="name" mode="selector" range="{{sexArray}}">
                        <input class="publish-list-input" type="text" disabled="{{true}}" placeholder="请选择性别" value="{{sexName}}" />
                    </picker>
                </view>
                <view class="publish-list-item"><text class="pulish-list-title">出生日期</text>
                    <picker mode="date" value="{{initModel ? initModel.memberExt.birthday : ''}}" bindchange="anonymousFunc4" start="{{startDate}}" end="{{endDate}}">
                        <input class="publish-list-input" type="text" disabled="{{true}}" placeholder="请选择出生日期" value="{{model && model.birthday}}" />
                    </picker>
                </view>
                <view class="publish-list-item"><text class="pulish-list-title">民族</text>
                    <picker mode="selector" value="{{nationCurrent}}" range-key="mz_name" range="{{initModel && initModel.nation || []}}" bindchange="anonymousFunc5">
                        <input class="publish-list-input" type="text" disabled="{{true}}" placeholder="请选择民族" value="{{initModel && initModel.nation[nationCurrent].mz_name}}" />
                    </picker>
                </view>
                <view class="publish-list-item"><text class="pulish-list-title">身份证号</text>
                    <input class="publish-list-input" type="text" placeholder="请输入身份证号码" maxlength="{{18}}" bindinput="anonymousFunc6" value="{{model && model.idCard}}" />
                </view>
                <view class="publish-list-item"><text class="pulish-list-title">详细地址</text>
                    <input class="publish-list-input" type="text" disabled="{{true}}" placeholder="请选择详细地址" bindtap="anonymousFunc7" value="{{model && model.address}}" />
                </view>
            </view>
            <block wx:if="{{model && !model.tel && checkDegree}}">
                <view class="realname-card-form">
                    <view class="publish-list-item"><text class="pulish-list-title">电话号码</text>
                        <input class="publish-list-input" type="text" placeholder="请输入电话号码" bindinput="anonymousFunc8" value="{{model && model.tel}}" />
                    </view>
                    <view class="publish-list-item publish-list-item-code"><text class="pulish-list-title">验证码</text>
                        <input class="publish-list-input" type="text" placeholder="请输入验证码" bindinput="anonymousFunc9" value="{{model && model.code}}" />
                        <view class="publish-code-btn" bindtap="anonymousFunc10">{{text}}</view>
                    </view>
                </view>
            </block>
            <button class="userauth-btn" bindtap="anonymousFunc11">身份认证</button>
        </view>
    </block>
</block>