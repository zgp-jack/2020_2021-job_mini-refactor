<block wx:if="{{$taroCompReady}}">
    <view class="integral-container">
        <view class="integral-title">显示最近30天的积分来源记录</view>
        <view class="integral-content">
            <view class="integral-list" wx:for="{{lists.lists}}" wx:for-item="item" wx:for-index="_anonIdx2">
                <view class="integral-list-time"><text class="integral-time-year">{{item.year_month}}</text><text class="integral-time-day">{{item.day}}</text>
                </view>
                <view class="integral-list-item">
                    <view class="integral-list-title overwords">{{item.headerText}}</view>
                    <view class="integral-list-words overwords">{{item.title}}</view>
                    <view class="integral-item-time">时间：{{item.His}}<text>{{item.expend_integral_string}}</text>
                    </view>
                </view>
            </view>
        </view>
        <block wx:if="{{!lists.hasmore}}">
            <view class="integral-notmore">没有更多数据了</view>
        </block>
    </view>
</block>