<block wx:if="{{$taroCompReady}}">
    <view class="content">
        <block wx:if="{{!data.item.length}}">
            <nodata></nodata>
        </block>
        <view class="content-box">
            <scroll-view class="recruit-lists-containerbox" scroll-y="{{true}}" lower-threshold="{{200}}" refresher-enabled="{{true}}" bindscrolltolower="anonymousFunc0">
                <view wx:key="id" class="recruitListBox" bindtap="anonymousFunc1" wx:for="{{loopArray95}}" wx:for-item="v" wx:for-index="__index1" data-e-tap-so="this" data-e-tap-a-a="{{v._$indexKey}}">
                    <view class="recruitListBox-title">{{v.$original.title}}</view>
                    <view class="recruitListBox-content">
                        <image src="{{v.$original.image}}" class="recruitListBox-content-img"></image>
                        <view class="recruitListBox-content-box">
                            <view class="recruitListBox-content-name">{{v.$original.user_name}}</view>
                            <view class="recruitListBox-content-details">{{v.$original.detail}}</view>
                        </view>
                    </view>
                    <view class="recruitListBox-footer">
                        <view>
                            <image class="recruitListBox-footer-image" src="{{IMGCDNURL + 'newlist-jobposi.png'}}"></image>{{v.$original.show_address}}</view>
                        <view>{{v.$original.time}}</view>
                    </view>
                </view>
                <block wx:if="{{!isDown && data.item.length}}">
                    <view class="footer" bindtap="anonymousFunc2">
                        <view class="footer-btn">查看更多</view>
                    </view>
                </block>
            </scroll-view>
        </view>
    </view>
</block>