<block wx:if="{{$taroCompReady}}">
    <view>
        <at-drawer compid="{{$compid__88}}">
            <view class="common-drawer-item">
                <scroll-view class="drawer-full-lists" scroll-y="{{true}}">
                    <block wx:if="{{initModel}}">
                        <view class="{{item.$loopState__temp2}}" wx:key="id" bindtap="anonymousFunc1" wx:for="{{loopArray85}}" wx:for-item="item" wx:for-index="index" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey}}">{{item.$original.name}}</view>
                    </block>
                </scroll-view>
                <block wx:if="{{initModel}}">
                    <scroll-view class="drawer-full-lists drawer-half-lists" scroll-y="{{true}}">
                        <view wx:key="id" class="{{item.$loopState__temp4}}" bindtap="anonymousFunc2" wx:for="{{loopArray86}}" wx:for-item="item" wx:for-index="k" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey2}}">{{item.$original.name}}</view>
                    </scroll-view>
                </block>
            </view>
        </at-drawer>
        <view class="publish-header">
            <view class="publish-title">发布二手交易</view>
            <view class="publish-header-title">请完善以下信息</view>
        </view>
        <view class="publish-recruit-container publish-used-container">
            <form>
                <view class="publish-recruit-card">
                    <view class="publish-list-item"><text class="pulish-list-title">标题</text>
                        <input class="publish-list-input" type="text" placeholder="请输入标题（3-30个字)" value="{{model.title}}" bindinput="anonymousFunc3" />
                    </view>
                    <view class="publish-list-item" bindtap="anonymousFunc4"><text class="pulish-list-title">交易目的</text>
                        <block>
                            <block wx:if="{{classifyName}}">
                                <view class="publish-list-input"><text class="publish-input-list">{{classifyName}}</text>
                                </view>
                            </block>
                            <block wx:else>
                                <input class="publish-list-input" disabled="{{true}}" type="text" placeholder="请选择交易目的" />
                            </block>
                        </block>
                    </view>
                    <view class="publish-list-item"><text class="pulish-list-title">详细地址</text>
                        <picker mode="multiSelector" range="{{[areaProvince, areaCity]}}" value="{{[pIndex, cIndex]}}" range-key="name" bindchange="anonymousFunc5" bindcolumnchange="anonymousFunc6">
                            <input class="publish-list-input" type="text" disabled="{{true}}" placeholder="请输入详细地址" value="{{cityName}}" />
                        </picker>
                    </view>
                    <view class="publish-list-item"><text class="pulish-list-title">联系人</text>
                        <input class="publish-list-input" type="text" placeholder="请输入联系人" value="{{model.user_name}}" bindinput="anonymousFunc7" />
                    </view>
                    <view class="publish-list-item"><text class="pulish-list-title">联系电话</text>
                        <input class="publish-list-input" type="number" placeholder="请输入联系电话" value="{{model.user_mobile}}" bindinput="anonymousFunc8" />
                    </view>
                    <block wx:if="{{userTel !== model.user_mobile || model.user_mobile == ''}}">
                        <view class="publish-list-item publish-list-item-code"><text class="pulish-list-title">验证码</text>
                            <input class="publish-list-input" type="text" placeholder="请输入验证码" value="{{model.code}}" bindinput="anonymousFunc9" />
                            <view class="publish-code-btn" bindtap="anonymousFunc10">{{text}}</view>
                        </view>
                    </block>
                    <view class="publish-list-textarea"><text class="publish-textarea-title">招工详情</text>
                        <textarea class="publish-textarea" value="{{model.detail}}" placeholder="{{initModel && initModel.placeholder}}" bindinput="anonymousFunc11"></textarea>
                        <words-total compid="{{$compid__89}}"></words-total>
                    </view>
                </view>
                <view class="publish-recruit-btn" bindtap="anonymousFunc12">确认发布</view>
            </form>
        </view>
    </view>
</block>