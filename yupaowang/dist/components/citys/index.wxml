<block wx:if="{{$taroCompReady}}">
    <view class="citiesbox">
        <view class="city-header">
            <view class="city-header-input">
                <input placeholder="请输入城市名" bindfocus="anonymousFunc0" bindinput="anonymousFunc1" value="{{text}}" />
                <image class="city-header-clear" bindtap="anonymousFunc2" src="{{IMGCDNURL + 'clear-input.png'}}"></image>
            </view><text class="city-header-close" bindtap="anonymousFunc3">取消</text>
        </view>
        <view class="city-area-tips">当前选择城市：{{area}}</view>
        <view class="city-area-container">
            <view class="city-area-item">
                <view class="city-area-title">当前定位城市/最近访问</view>
                <view class="city-area-content clearfix">
                    <block wx:if="{{userLoc.id}}">
                        <view class="city-item">
                            <view class="city-item-posi">
                                <image class="city-item-img" src="{{IMGCDNURL + 'gps-posi.png'}}"></image>{{userLoc.city}}</view>
                        </view>
                    </block>
                    <view class="city-item" wx:key="$loopState__temp2" bindtap="anonymousFunc4" wx:for="{{loopArray43}}" wx:for-item="item" wx:for-index="__index4" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey}}"><text class="city-item-text overwords">{{item.$original.city}}</text>
                    </view>
                </view>
            </view>
            <view class="city-area-item" wx:key="$loopState__temp4" wx:for="{{loopArray44}}" wx:for-item="item" wx:for-index="index">
                <view class="city-area-title">{{item.$original[0].city}}</view>
                <view class="city-area-content clearfix">
                    <view class="city-item" wx:key="$loopState__temp6" bindtap="anonymousFunc5" wx:for="{{item.$anonymousCallee__8}}" wx:for-item="d" wx:for-index="key" data-e-tap-so="this" data-e-tap-a-a="{{d._$indexKey2}}"><text class="city-item-text overwords">{{d.$original.city}}</text>
                    </view>
                </view>
            </view>
        </view>
        <block wx:if="{{show}}">
            <view class="inputlist">
                <block>
                    <block wx:if="{{inputCity.length}}">
                        <view class="input-list-item" bindtap="anonymousFunc6" wx:key="id" wx:for="{{loopArray45}}" wx:for-item="item" wx:for-index="__index6" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey3}}">{{item.$original.city_name}}</view>
                    </block>
                    <block wx:else>
                        <view class="notsearchdata">
                            <image class="notimg" src="{{IMGCDNURL + 'nodata.png'}}"></image><text class="nottext">暂无数据</text>
                        </view>
                    </block>
                </block>
            </view>
        </block>
    </view>
</block>