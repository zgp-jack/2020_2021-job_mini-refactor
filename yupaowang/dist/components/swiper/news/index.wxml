<block wx:if="{{$taroCompReady}}">
    <swiper class="home-swipernewsbox" vertical="{{swiper.vertical}}" circular="{{swiper.circular}}" indicator-dots="{{swiper.indicatorDots}}" autoplay="{{swiper.autoplay}}">
        <swiper-item class="home-swipernews-item" wx:key="index" wx:for="{{swiper.lists}}" wx:for-item="item" wx:for-index="index">
            <view class="home-swipernews-text overwords">{{item.text}}</view>
        </swiper-item>
    </swiper>
</block>