<block wx:if="{{$taroCompReady}}">
    <view class="resume-list-container">
        <view class="resumeDetail-recommend">
            <view class="resumeDetail-recommend-top"><text class="resumeDetail-recommend-top-text">相关推荐</text>
            </view>
        </view>
        <block wx:key="id" wx:for="{{loopArray105}}" wx:for-item="item" wx:for-index="__index0">
            <view class="resume-list-item" wx:key="id" bindtap="anonymousFunc0" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey}}">
                <view class="resume-list-header">
                    <image class="resume-list-user" src="{{item.$original.headerimg}}"></image>
                    <view class="resume-list-userinfo">
                        <view class="resume-list-userinfo-detail"><text class="resume-userinfo-name">{{item.$original.username}}</text><text class="resume-userinfo-birthday">{{item.$original.birthday}}岁</text>
                            <block wx:if="{{item.$original.certificate == 1}}">
                                <image class="resume-userinfo-img" src="{{IMGCDNURL + 'newresume-infolist-jnz.png?t=1'}}"></image>
                            </block>
                        </view><text class="resume-list-type">{{item.$original.type}}</text>
                        <view class="resume-otherinfo"><text class="resume-otherinfo-text">{{item.$original.nation}}</text><text class="resume-otherinfo-text">{{item.$original.experience}}</text>
                        </view>
                    </view>
                </view>
                <view class="resume-list-works"><text class="resume-list-words-text" wx:key="$loopState__temp2" wx:for="{{item.$anonymousCallee__21}}" wx:for-item="v" wx:for-index="i">{{v.$original}}</text>
                </view>
                <view class="resume-list-content overwords">{{item.$original.introduce}}</view>
                <view class="resume-list-footer">
                    <view class="resume-list-footer-flex">
                        <image class="resume-list-loc" src="{{IMGCDNURL + 'lpy/biaoqian.png'}}"></image>
                        <view class="resume-list-loctxt overwords">{{item.$original.show_address}}</view>
                    </view><text class="resume-list-time">{{item.$original.prof_degree}}</text>
                </view>
            </view>
        </block>
        <view class="seemore-recommend-recruit">查看更多找活信息</view>
    </view>
</block>