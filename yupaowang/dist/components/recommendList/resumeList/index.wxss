.overwords {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; }
.showMore {
  position: relative;
  width: 100%;
  padding: 20rpx 0;
  text-align: center;
  font-size: 26rpx;
  color: #525252; }
.clearfix:after {
  content: "";
  display: block;
  clear: both;
  visibility: hidden;
  line-height: 0;
  height: 0;
  font-size: 0; }
.clearfix {
  zoom: 1; }
.resume-list-container {
