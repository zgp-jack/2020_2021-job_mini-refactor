<block wx:if="{{$taroCompReady}}">
    <view class="resume-list-container" style="{{anonymousState__temp}}">
        <block wx:for="{{data}}" wx:for-item="item" wx:for-index="_anonIdx6">
            <view class="resume-list-item" wx:key="id" wx:for="{{item}}" wx:for-item="d" wx:for-index="_anonIdx4">
                <view class="resume-list-header">
                    <image class="resume-list-user" src="{{d.headerimg}}"></image>
                    <view class="resume-list-userinfo">
                        <view class="resume-list-userinfo-detail"><text class="resume-userinfo-name">{{d.username}}</text><text class="resume-userinfo-sex">男</text><text class="resume-userinfo-age">{{d.birthday}}岁</text>
                        </view><text class="resume-list-type">{{d.type}}</text>
                        <view class="resume-otherinfo"><text class="resume-otherinfo-text">{{d.nation}}</text><text class="resume-otherinfo-text">{{d.experience}}</text>
                        </view>
                    </view>
                </view>
                <view class="resume-list-works"><text class="resume-list-words-text" wx:for="{{d.occupations}}" wx:for-item="i" wx:for-index="_anonIdx2">{{i}}</text>
                </view>
                <view class="resume-list-content overwords">{{d.introduce}}</view>
                <view class="resume-list-footer">
                    <image class="resume-list-loc" src="{{IMGCDNURL + 'lpy/biaoqian.png'}}"></image>
                    <view class="resume-list-loctxt overwords">{{d.show_address}}</view><text class="resume-list-time">{{d.prof_degree}}</text>
                </view>
            </view>
        </block>
    </view>
</block>