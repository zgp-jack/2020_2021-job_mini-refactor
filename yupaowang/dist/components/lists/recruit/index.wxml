<block wx:if="{{$taroCompReady}}">
    <view class="recruit-lists-container" style="{{anonymousState__temp}}">
        <block wx:if="{{data}}">
            <block wx:for="{{loopArray109}}" wx:for-item="item" wx:for-index="__index1">
                <block wx:if="{{item.$original}}">
                    <view class="recruit-list-item" wx:key="id" bindtap="anonymousFunc0" wx:for="{{item.$anonymousCallee__22}}" wx:for-item="d" wx:for-index="__index0" data-e-tap-so="this" data-e-tap-a-a="{{d._$indexKey}}">
                        <block wx:if="{{d.$original.top}}">
                            <image class="list-toprecruit-img" src="{{IMGCDNURL + 'newlist-jobzd.png'}}"></image>
                        </block>
                        <block wx:if="{{d.$original.is_end == 2}}">
                            <image class="recruit-findend-img" src="{{IMGCDNURL + 'newlist-jobfindend.png'}}"></image>
                        </block>
                        <view class="recruit-list-header">
                            <view class="recruit-list-title overwords">{{d.$original.title}}</view>
                            <block wx:if="{{d.$original.top}}"><text class="recruit-list-settop">我要置顶</text>
                            </block>
                        </view>
                        <view class="recruit-list-body">
                            <image class="recruit-list-user" src="{{d.$original.image}}"></image>
                            <view class="recruit-list-body-content">
                                <view class="recruit-body-header"><text class="recruit-list-username">{{d.$original.user_name}}</text>
                                    <block wx:if="{{d.$original.is_end !== 2}}">
                                        <image class="recruit-list-finding" src="{{IMGCDNURL + 'newlist-jobfinding.png'}}"></image>
                                    </block>
                                </view>
                                <view class="recruit-list-words">
                                    <view class="recruit-words-text">{{d.$original.detail}}</view>
                                </view>
                            </view>
                        </view>
                        <view class="recruit-list-footer">
                            <image class="recruit-list-loc" src="{{IMGCDNURL + 'newlist-jobposi.png'}}"></image>
                            <view class="recruit-list-loctxt overwords">{{d.$original.show_address}}</view><text class="recruit-list-time">{{d.$original.time}}</text>
                        </view>
                    </view>
                </block>
            </block>
        </block>
    </view>
</block>